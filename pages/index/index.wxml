<view>
  <!-- 搜索框 -->
  <SearchInput tabItems="{{tabs}}" bindmytap="onMyTap" />
  <!-- 轮播图 -->
  <view class="index_swiper">
    <swiper indicator-dots="true" autoplay="true" circular="true" class="swiper1">
      <swiper-item wx:for='{{swiper_list}}' wx:key='good_id'>
        <navigator url="{{item.navigator_url}}" open-type="{{item.open_type}}">
          <image src="{{item.image_src}}" mode="widthFix"></image>
        </navigator>
      </swiper-item>
    </swiper>
  </view>

  <!-- 导航栏 -->
  <view class="nav">
    <navigator wx:for="{{navs}}" wx:key="name" url="#" open-type="switchTab">
      <image mode="widthFix" src="{{item.image_src}}"></image>
    </navigator>
  </view>
  <!-- 时尚女装标题-->
  <view class="fashion">
    <view class="floor_group">
      <!-- 时尚女装标题开始-->
      <block wx:for="{{floors}}" wx:key="index">
        <view class="fashion1">
          <image src="{{item.floor_title.image_src}}" mode="widthFix"></image>
        </view>
        <!-- 时尚女装内容开始-->
        <view class="floor_list">
          <block wx:for="{{item.product_list}}" wx:key="i" wx:for-item="v" wx:for-index="i">
            <navigator url="{{v.navigator_url}}" open-type="{{v.open_type}}">
              <image src="{{v.image_src}}" mode="{{ i===0 ?'widthFix': 'scaleToFill' }}"></image>
            </navigator>
          </block>
        </view>
      </block>
    </view>
  </view>


</view>